<playlist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../TestConfig/playlist.xsd">
	<test>
		<testCaseName>SanityTestExample</testCaseName>
		<variations>
			<variation>-Xint</variation>
		</variations>
		<command>$(JAVA_COMMAND) $(JVM_OPTIONS) \
	-cp $(Q)$(RESOURCES_DIR)$(P)$(TEST_RESROOT)$(D)testng.jar$(P)$(TEST_RESROOT)$(D)jcommander.jar$(P)$(TEST_RESROOT)$(D)example.jar$(Q) \
	org.testng.TestNG -d $(REPORTDIR) $(Q)$(TEST_RESROOT)$(D)testng.xml$(Q) -testnames exampleTest \
	-groups $(TEST_GROUP) \
	-excludegroups $(DEFAULT_EXCLUDE); \
	$(TEST_STATUS)</command>
		<platformRequirements>os.linux,arch.x86,bits.64</platformRequirements>
		<tags>
			<tag>example</tag>
		</tags>
		<subsets>
			<subset>SE80</subset>
			<subset>SE90</subset>
		</subsets>
	</test>
		<test>
		<testCaseName>ExtendedTestExample</testCaseName>
		<variations>
			<variation>-Xint</variation>
		</variations>
		<command>$(JAVA_COMMAND) $(JVM_OPTIONS) \
	-cp $(Q)$(RESOURCES_DIR)$(P)$(TEST_RESROOT)$(D)testng.jar$(P)$(TEST_RESROOT)$(D)jcommander.jar$(P)$(TEST_RESROOT)$(D)example.jar$(Q) \
	org.testng.TestNG -d $(REPORTDIR) $(Q)$(TEST_RESROOT)$(D)testng.xml$(Q) -testnames exampleTest \
	-groups $(TEST_GROUP) \
	-excludegroups $(DEFAULT_EXCLUDE); \
	$(TEST_STATUS)</command>
		<platformRequirements>os.linux,arch.x86,bits.64</platformRequirements>
		<tags>
			<tag>example</tag>
		</tags>
		<subsets>
			<subset>SE80</subset>
			<subset>SE90</subset>
		</subsets>
	</test>
</playlist>