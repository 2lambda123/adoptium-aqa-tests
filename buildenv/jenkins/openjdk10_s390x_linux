node('linux&&s390x&&test') {
    PLATFORM = 's390x_linux'
    JAVA_VERSION = 'SE100'
    SDK_RESOURCE = 'upstream'
    CUSTOMIZED_SDK_URL = ''
    SPEC='linux_390-64'
    checkout scm
    def jenkinsfile = load "${WORKSPACE}/openjdk-tests/buildenv/jenkins/JenkinsfileBase"
    jenkinsfile.test()
}